{
  "name": "@auth/server",
  "version": "0.0.0",
  "author": "lewis-wow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/server"
  },
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "run-s build:tsup build:tsc",
    "build:tsup": "tsup",
    "build:tsc": "tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "typings": "dist/main.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "!**/*.test.*"
  ],
  "devDependencies": {
    "@auth/tsconfig": "workspace:*",
    "@auth/tsup": "workspace:*",
    "@types/node": "^22.5.2",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@whatwg-node/server": "^0.9.49",
    "arctic": "^1.9.2",
    "itty-router": "^5.0.18",
    "lucia": "^3.2.0",
    "oslo": "^1.2.1"
  }
}
